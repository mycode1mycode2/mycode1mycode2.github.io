age = int(input("How old are you? "))
year = int(input("What year is it? "))
question1 = str(input("Has your birthaday happend yet? Say yes or no! "))

if str(question1) == "yes" or str(question1) == "Yes":
    yes = int(year) - int(age)
    print("You were born in " + str(yes))
    
elif str(question1) == "no" or str(question1) == "No":
    no = int(year) - (int(age) + 1)
    print("You were born in " + str(no))
